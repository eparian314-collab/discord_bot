# Environment and secrets
.env
.env.*
*.key
*.pem
config/config.json
config/*.secret.json

# Python cache / compiled files
__pycache__/
*.py[cod]
*$py.class

# Virtual environments
venv/
.venv/
env/
ENV/
env.bak/
pip-wheel-metadata/
dist/
build/

# Logs and database files
*.log
*.sqlite
*.db
*.db-journal
*.sqlite3
/core/storage/baby_hippo.db
/core/storage/*.json
/cache/
*.bak
health_check.log

# OS-specific files
.DS_Store
Thumbs.db
desktop.ini

# IDE / Editor settings
.vscode/
.vs/
.idea/
*.suo
*.user
*.userosscache
*.sln.docstates

# Test and temporary output
.pytest_cache/
coverage/
*.cover
htmlcov/
.cache/
tmp/
temp/

# Distribution and packaging
*.egg
*.egg-info/
.eggs/
MANIFEST

# GitHub / CI
.github/workflows/__pycache__/
.gitignore.bak
# Ignore workflow-related prompt files
.github/prompts/
.github/prompts/**
prompts/
prompts/**

# Ignore WSL virtual environment
.wslvenv/

# Documentation and AI workflow files
# Architecture and implementation docs
worfklow/architecture/
worfklow/deployment/
worfklow/md/
worfklow/instructions/*.md

# Logs and diagnostics
logs/diagnostics/
logs/screenshots/

# Scripts and utilities
scripts/diagnostics/
scripts/deployment/
scripts/utilities/

# Project files
config/project/

